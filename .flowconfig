[ignore]
.*\.yarn/.*
.*\/cache\/Cypress\/.*

[include]

[libs]
config/flow-typed

[lints]
deprecated-utility=warn

[options]
esproposal.optional_chaining=enable

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null

[untyped]
<PROJECT_ROOT>/node_modules/redux-persist/lib/integration/react.js.flow
<PROJECT_ROOT>/node_modules/redux-persist/lib/persistReducer.js.flow
<PROJECT_ROOT>/node_modules/draft-js/lib/DraftEditorDragHandler.js.flow
<PROJECT_ROOT>/node_modules/draft-js/lib/DraftEditor.react.js.flow
<PROJECT_ROOT>/node_modules/draft-js/lib/convertFromHTMLToContentBlocks.js.flow
<PROJECT_ROOT>/node_modules/draft-js/node_modules/fbjs/lib/emptyFunction.js.flow
