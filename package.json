{
  "name": "chop-web-client",
  "version": "3.0.28",
  "description": "Church Online Platform (ChOP) web client",
  "main": "index.tsx",
  "repository": "git@in.thewardro.be:kenny.heaton/chop-web-client.git",
  "author": "chop@life.church",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build:review": "webpack --config config/webpack.review.js",
    "build:staging": "webpack --config config/webpack.staging.js",
    "build:production": "webpack --config config/webpack.prod.js",
    "start": "webpack-dev-server --open --config config/webpack.dev.js",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "flow": "flow",
    "flow:coverage": "yarn flow-coverage-report",
    "flow:search": "yarn flow-typed search",
    "flow:install": "yarn flow-typed install",
    "lint": "eslint -c config/.eslintrc.json --ext .jsx,.js src/ test/ cypress/",
    "validate": "yarn test:coverage && yarn flow && yarn lint",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run",
    "cy:coverage": "nyc report"
  },
  "jest": {
    "testRegex": "/test/unit/.*\\.test\\.jsx?$",
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "iframe_api": "<rootDir>/test/fake-js-file.js",
      "Youtube\\.min\\.js$": "<rootDir>/test/fake-js-file.js"
    },
    "testURL": "https://live.life.church",
    "setupFilesAfterEnv": [
      "jest-extended",
      "react-testing-library/cleanup-after-each"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 82,
        "branches": 63,
        "functions": 75,
        "lines": 82
      }
    },
    "globals": {
      "ENV": "test",
      "GATEWAY_HOST": "https://staging.chopapi.com/graphql",
      "GTM": {
        "gtmId": "GTM-XXXXXXX",
        "auth": "XXXXXXXXXXXXXXXXXX",
        "preview": "env-X"
      },
      "CWC_HOST": ""
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "test/testUtils/",
      "/test/fileMock.js",
      "/src/index.jsx",
      "/src/styles.js",
      "/src/util/bugsnag.js"
    ]
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js",
      "src/**/*.jsx",
      "test/**/*.js",
      "test/**/*.jsx"
    ],
    "threshold": 70,
    "reportTypes": [
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@welldone-software/why-did-you-render": "^3.0.6",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "^1.10.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.97.0",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-extended": "^0.11.1",
    "jest-snapshot": "^24.7.1",
    "jest-styled-components": "^6.3.1",
    "nyc": "^14.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "sugarss": "^2.0.0",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.30.0",
    "webpack-bugsnag-plugins": "^1.4.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@bugsnag/browser": "^6.2.0",
    "@bugsnag/js": "^6.2.0",
    "@bugsnag/plugin-react": "^6.2.0",
    "@redux-beacon/google-tag-manager": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "bezier-easing": "^2.1.0",
    "color": "^3.1.1",
    "core-js": "^3.0.1",
    "dayjs": "^1.8.12",
    "dompurify": "^1.0.10",
    "graphql-request": "^1.8.2",
    "i18next": "^15.1.0",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "linkifyjs": "^2.1.8",
    "object-hash": "^1.3.1",
    "prettier": "^1.17.0",
    "pubnub": "^4.23.0",
    "react": "^16.8.6",
    "react-countdown-now": "^2.1.0",
    "react-dom": "^16.8.6",
    "react-gtm-module": "^2.0.4",
    "react-i18next": "^10.9.0",
    "react-player": "^1.11.0",
    "react-redux": "^7.0.2",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^5.0.0",
    "react-spring": "^8.0.19",
    "react-touch-events": "^2.1.0",
    "redux": "^4.0.1",
    "redux-beacon": "^2.1.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-expire": "^0.0.2",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0",
    "styled-css-grid": "^1.2.1",
    "styled-reset": "^2.0.8"
  }
}
