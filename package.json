{
  "name": "chop-web-client",
  "version": "3.0.28",
  "description": "Church Online Platform (ChOP) web client",
  "main": "index.tsx",
  "repository": "git@in.thewardro.be:kenny.heaton/chop-web-client.git",
  "author": "chop@life.church",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build:review": "webpack --config config/webpack.review.js",
    "build:staging": "webpack --config config/webpack.staging.js",
    "build:production": "webpack --config config/webpack.prod.js",
    "start": "webpack-dev-server --open --config config/webpack.dev.js",
    "server": "node ./node_modules/.bin/ez-s ./dist",
    "functional": "jest --testRegex='/test/functional/.*\\.test\\.jsx?$'",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "flow": "flow",
    "lint": "eslint -c config/.eslintrc.json --ext .jsx,.js src/ test/functional/ test/unit/",
    "validate": "yarn test && yarn flow && yarn lint",
    "test:full": "yarn test && yarn flow && yarn lint && yarn cy:run",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run"
  },
  "jest": {
    "testRegex": "/test/unit/.*\\.test\\.jsx?$",
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "iframe_api": "<rootDir>/test/fake-js-file.js",
      "Youtube\\.min\\.js$": "<rootDir>/test/fake-js-file.js"
    },
    "testURL": "https://live.life.church",
    "setupFiles": [
      "<rootDir>/test/setupFile.js"
    ]
  },
  "devDependencies": {
    "@alshakero/ez-s": "^0.0.3",
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-styled-components": "^1.10.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.8",
    "cypress": "3.1.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "0.92.1",
    "flow-typed": "2.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-snapshot": "^24.0.0",
    "jest-styled-components": "^6.3.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.1",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "sugarss": "^1.0.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.27.1",
    "webpack-bugsnag-plugins": "^1.2.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@bugsnag/browser": "^5.0.2",
    "@bugsnag/js": "^5.0.2",
    "@bugsnag/plugin-react": "^5.0.2",
    "@redux-beacon/google-tag-manager": "^1.0.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "bezier-easing": "^2.1.0",
    "bugsnag-react": "^1.1.1",
    "dompurify": "^1.0.8",
    "git-revision-webpack-plugin": "^3.0.3",
    "graphql-request": "^1.8.2",
    "graphql.js": "^0.4.20",
    "gsap": "^2.0.2",
    "linkifyjs": "^2.1.7",
    "moment": "^2.22.2",
    "pubnub": "^4.21.6",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-gtm-module": "^2.0.2",
    "react-player": "^1.9.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-touch-events": "^2.1.0",
    "redux": "^4.0.1",
    "redux-beacon": "^2.0.5",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-expire": "^0.0.2",
    "redux-persist-transform-filter": "^0.0.18",
    "reselect": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.3",
    "styled-components": "^4.1.3",
    "styled-reset": "^1.6.8"
  }
}
