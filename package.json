{
  "name": "chop-web-client",
  "version": "0.16.0",
  "description": "Church Online Platform (ChOP) web client",
  "main": "index.tsx",
  "repository": "git@in.thewardro.be:kenny.heaton/chop-web-client.git",
  "author": "chop@life.church",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js && yarn export-storybook",
    "start": "webpack-dev-server --open --config config/webpack.dev.js",
    "server": "node ./node_modules/.bin/ez-s ./dist",
    "functional": "jest --testRegex='/test/functional/.*\\.test\\.jsx?$'",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "snap": "jest --testRegex='/test/snapshots/.*\\.test\\.jsx?$'",
    "flow": "flow",
    "storybook": "start-storybook -p 9001 -c config/.storybook",
    "lint": "eslint -c config/.eslintrc.json --ext .jsx,.js src/ test/functional/ test/unit/",
    "validate": "yarn test && yarn flow && yarn lint && yarn snap",
    "ftest": "yarn test && yarn flow && yarn lint",
    "plato": "es6-plato -r -d ./report src",
    "export-storybook": "build-storybook -c ./config/.storybook -o ./dist/storybook"
  },
  "jest": {
    "testRegex": "/test/unit/.*\\.test\\.jsx?$",
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(svg)$": "<rootDir>/test/svg-stub.js",
      "Youtube\\.min\\.js$": "<rootDir>/test/fake-js-file.js"
    }
  },
  "devDependencies": {
    "@alshakero/ez-s": "^0.0.3",
    "@storybook/addon-storyshots": "^3.4.1",
    "@storybook/addons": "3.4.1",
    "@storybook/react": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-plato": "^1.0.18",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "flow-typed": "^2.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.21.0",
    "sugarss": "^1.0.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@redux-beacon/google-tag-manager": "^1.0.1",
    "bezier-easing": "^2.1.0",
    "bugsnag-js": "^4.7.3",
    "bugsnag-react": "^1.1.1",
    "chat-engine": "^0.9.5",
    "graphql.js": "^0.4.20",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-gtm-module": "^2.0.2",
    "react-redux": "^5.0.7",
    "react-touch-events": "^2.1.0",
    "redux": "^3.7.2",
    "redux-beacon": "^2.0.3",
    "reselect": "^4.0.0",
    "reset-css": "^3.0.0",
    "video.js": "^7.2.4",
    "videojs-youtube": "^2.6.0"
  }
}
